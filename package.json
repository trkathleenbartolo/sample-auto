  "main": "nightwatch.conf.js",
  "scripts": {
    "e2e": "nightwatch",
    "e2e:chrome": "npm run e2e -- --env chrome",
    "e2e:firefox": "npm run e2e -- --env firefox",
    "debug": "node --inspect-brk node_modules/.bin/nightwatch --env chrome",
    "test": "nightwatch --tag test",
    "test:ff": "npm run e2e -- --env firefox --tag test",
    "smoketest": "nightwatch --group smoketest",
    "public-pages": "nightwatch --group public_pages"
  },
